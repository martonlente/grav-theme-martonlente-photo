{
  "name": "grav-theme-martonlente-photo",
  "version": "1.0.0",
  "description": "The **Grav Theme Martonlente Photo** Theme is for [Grav CMS](http://github.com/getgrav/grav).  This README.md file should be modified to describe the features, installation, configuration, and general usage of this theme.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "npm-run-all css-compile css-prefix",
    "css-compile": "sass --no-source-map scss:css-compiled",
    "css-prefix": "postcss css-compiled/*.css --use autoprefixer --no-map -d css-compiled/",
    "watch": "nodemon --exec \"npm run css\" --ext scss --watch scss"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "isotope-layout": "^3.0.6",
    "reset-css": "^5.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "sass": "^1.81.0"
  }
}
